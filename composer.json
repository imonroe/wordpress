{
  "name": "imonroe/wordpress",
  "description": "A basic wordpress starter config.",
  "authors": [
    {
        "name": "Ian Monroe",
        "email": "ian@ianmonroe.com"
    }
  ],
  "repositories": [
    {
        "type":"composer",
        "url":"https://wpackagist.org"
    },
    {
        "type": "package",
        "package": {
            "name": "wordpress/wordpress",
            "type": "webroot",
            "version": "4.8.1",
            "source": {
                "type": "git",
                "url": "https://github.com/WordPress/WordPress.git",
                "reference": "4.8.1"
            },
            "require": {
                "fancyguy/webroot-installer": "1.0.0"
            }
        }
    }
 ],
    "require": {
      "php": ">=5.3.0",
      "composer/installers": "~1.0",
      "fancyguy/webroot-installer":">=1.1.0",
      "wordpress": ">=4.8.1",
      "wpackagist-plugin/akismet":"dev-trunk",
      "wpackagist-plugin/captcha":">=3.9",
      "wpackagist-plugin/wordfence": "6.3.*",
      "wpackagist-plugin/woocommerce": "3.1.*",
      "wpackagist-plugin/tailor": "dev-trunk",
      "wpackagist-plugin/tailor-advanced": "dev-trunk",
      "wpackagist-plugin/tailor-portfolio": "dev-trunk",
      "wpackagist-plugin/tailor-woocommerce": "dev-trunk",
      "wpackagist-theme/wp-bootstrap-starter": "2.4.8"
    },
    "extra": {
        "webroot-dir": ".",
        "webroot-package": "wordpress"
    }

}
